#%RAML 1.0
title: Test API
mediaType:
- application/json

types:

  ValidationTest:
    type: object
    properties:
      codes:
        type: array
        items:
          type: string
          minLength: 2
          maxLength: 5
          pattern: "^[A-Z]+$"

/validation:
  get:
    responses:
      200:
        body:
          type: ValidationTest
