#%RAML 1.0
title: Test API
mediaType:
- application/json

types:

  Test:
    type: object
    properties:
      value1: string
      value2: integer


/tests:
  put:
    displayName: putTest
    headers:
      Expect:
        type: string
        enum: [100-continue]
      x-custom:
        type: string
        required: true
    responses:
      200:
        body:
          type: Test