#%RAML 1.0
title: Test API
mediaType:
- application/json

types:

  Test:
    type: object
    properties:
      value1: string
      value2: integer


/tests:
  get:
    displayName: fetchTest
    headers:
      obj?: Test
      str?: string
      int: integer | nil
      def1?:
        type: string
        default: test
      def2:
        type: integer | nil
        default: 10
    responses:
      200:
        body:
          type: Test
