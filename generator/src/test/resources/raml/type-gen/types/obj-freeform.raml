#%RAML 1.0
title: Test API
mediaType:
- application/json

types:

  Map:
    type: object
    additionalProperties: true

  Array:
    type: array

  Test:
    type: object
    properties:
      map: Map
      array: Array

/tests/{id}:

  get:
    displayName: fetchTest
    responses:
      200:
        body: Test
