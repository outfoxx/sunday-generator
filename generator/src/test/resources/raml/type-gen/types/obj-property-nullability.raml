#%RAML 1.0
title: Test API
mediaType:
- application/json

types:

  Test:
    type: object
    properties:
      fromNilUnion: (string | nil)
      notRequired:
        required: false
        type: string

  Test2:
    type: object
    properties:
      optionalObject?: object
      nillableObject: object?

/tests/{id}:

  get:
    displayName: fetchTest
    responses:
      200:
        body: Test | Test2
