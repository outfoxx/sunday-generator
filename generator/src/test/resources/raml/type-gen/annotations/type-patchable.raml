#%RAML 1.0
title: Test API
uses:
  sunday: https://outfoxx.github.io/sunday-generator/sunday.raml
mediaType:
- application/json

types:

  Test:
    type: object
    (sunday.patchable): true
    properties:
      string: string
      int: integer
      bool: boolean

/tests/{id}:

  get:
    displayName: fetchTest
    responses:
      200:
        body:
          type: Test
