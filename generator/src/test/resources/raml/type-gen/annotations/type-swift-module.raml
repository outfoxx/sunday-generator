#%RAML 1.0
title: Test API
uses:
  sunday: https://outfoxx.github.io/sunday-generator/sunday.raml
mediaType:
- application/json

(sunday.swiftModule): Explicit

types:

  Test:
    type: object
    properties:
      value: string

/tests/{id}:

  get:
    displayName: fetchTest
    responses:
      200:
        body: Test
