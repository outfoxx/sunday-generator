#%RAML 1.0
title: Test API
uses:
  sunday: https://outfoxx.github.io/sunday-generator/sunday.raml
mediaType:
- application/json

types:

  Value:
    type: string
    (sunday.kotlinType): java.time.Instant
    (sunday.kotlinType:client): java.time.LocalDateTime

  Test:
    type: object
    properties:
      value: Value

/tests/{id}:

  get:
    displayName: fetchTest
    responses:
      200:
        body:
          type: Test
