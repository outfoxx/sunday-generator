#%RAML 1.0
title: Test API
mediaType:
- application/json

types:

  Test:
    type: object
    properties:
      labels:
        type: object
        properties:
          /^[a-z]+$/: 
            type: string
            minLength: 2
            maxLength: 5
            pattern: "^[a-z]+$"

/tests/{id}:

  get:
    displayName: fetchTest
    responses:
      200:
        body:
          type: Test
