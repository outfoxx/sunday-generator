#%RAML 1.0
title: Test API
uses:
  sunday: https://outfoxx.github.io/sunday-generator/sunday.raml
mediaType:
- application/json

types:

  Test:
    type: object
    properties:
      minList:
        type: array
        items: string
        uniqueItems: false
        minItems: 5
      maxList:
        type: array
        items: string
        uniqueItems: false
        maxItems: 10
      minSet:
        type: array
        items: string
        uniqueItems: true
        minItems: 15
      maxSet:
        type: array
        items: string
        uniqueItems: true
        maxItems: 20

/tests/{id}:

  get:
    displayName: fetchTest
    responses:
      200:
        body:
          type: Test
